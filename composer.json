{
    "name": "quibax/laravel-singlestore-grammar",
    "description": "SingleStore compatible grammar for Eloquent",
    "keywords": [
        "QuibaX",
        "laravel",
        "laravel-singlestore-grammar"
    ],
    "homepage": "https://github.com/quibax/laravel-singlestore-grammar",
    "license": "MIT",
    "authors": [
        {
            "name": "QuibaX",
            "email": "quibax@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37|^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1|^1.2",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "QuibaX\\LaravelSinglestoreGrammar\\": "src"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
